        "custom_rG8DNm": {
          "type": "custom",
          "settings": {
            "code": "<!-- ✅ Dynamic Stock Indicator (random 2–5 per color/size in same session, SOLD OUT when unavailable) -->\n<div class=\"stock-indicator\" id=\"stock-indicator\">\n  <span class=\"dot\" id=\"stock-dot\"></span>\n  <span id=\"stock-text\">Only <strong id=\"stock-number\"></strong> left in stock!</span>\n</div>\n\n<style>\n  .stock-indicator{\n    display:flex;\n    align-items:center;\n    font-family:inherit;\n    color:#000;\n    font-size:14px;\n    font-weight:500;\n  }\n  .stock-indicator .dot{\n    width:8px;height:8px;\n    background-color:#e60000;\n    border-radius:50%;\n    margin-right:8px;\n    animation:blink 2.5s infinite;\n  }\n  .stock-indicator.is-soldout .dot{ display:none; }\n\n  @keyframes blink{\n    0%,80%,100%{opacity:1;}\n    40%{opacity:.2;}\n  }\n</style>\n\n<script>\n(function () {\n  const MIN_STOCK = 2;\n  const MAX_STOCK = 5;\n\n  const PRODUCT_KEY = \"{{ product.handle | default: 'genericProduct' }}\";\n  const VARIANTS = {{ product.variants | json }};\n\n  function getSelectedOptionValue(optionIndex) {\n    // Swatches\n    const sw = document.querySelector(`.swatch-selector[swatch-option=\"option${optionIndex}\"].swatch-selected`);\n    if (sw) return (sw.getAttribute(\"data-value\") || sw.getAttribute(\"orig-value\") || \"\").trim();\n\n    // Dropdowns\n    const sel = document.querySelector(`select[name=\"options[option${optionIndex}]\"], select[data-option-position=\"${optionIndex}\"]`);\n    if (sel && sel.value) return sel.value.trim();\n\n    // Radios\n    const radio = document.querySelector(`input[name=\"option${optionIndex}\"]:checked, input[name*=\"option-${optionIndex}\"]:checked`);\n    if (radio && radio.value) return radio.value.trim();\n\n    return \"\";\n  }\n\n  function getVariantFromSelectedOptions() {\n    const o1 = getSelectedOptionValue(1);\n    const o2 = getSelectedOptionValue(2);\n    const o3 = getSelectedOptionValue(3);\n\n    const match = VARIANTS.find(v => {\n      const ok1 = o1 ? String(v.option1 || \"\").trim() === o1 : true;\n      const ok2 = o2 ? String(v.option2 || \"\").trim() === o2 : true;\n      const ok3 = o3 ? String(v.option3 || \"\").trim() === o3 : true;\n      return ok1 && ok2 && ok3;\n    });\n\n    return match || VARIANTS.find(v => v.available) || VARIANTS[0] || null;\n  }\n\n  function getKey(variant) {\n    const c = variant && variant.option1 ? String(variant.option1).trim() : \"noColor\";\n    const s = variant && variant.option2 ? String(variant.option2).trim() : \"noSize\";\n    return `dynamicStock_${PRODUCT_KEY}_${c}_${s}`;\n  }\n\n  function setSoldOutUI(isSoldOut) {\n    const container = document.getElementById(\"stock-indicator\");\n    const textEl = document.getElementById(\"stock-text\");\n\n    if (!container || !textEl) return;\n\n    if (isSoldOut) {\n      container.classList.add(\"is-soldout\");\n      textEl.textContent = \"SOLD OUT\";\n    } else {\n      container.classList.remove(\"is-soldout\");\n      textEl.innerHTML = 'Only <strong id=\"stock-number\"></strong> left in stock!';\n    }\n  }\n\n  function update() {\n    const container = document.getElementById(\"stock-indicator\");\n    if (!container) return;\n\n    const variant = getVariantFromSelectedOptions();\n    if (!variant) return;\n\n    if (!variant.available) {\n      setSoldOutUI(true);\n      return;\n    }\n\n    setSoldOutUI(false);\n\n    const stockEl = document.getElementById(\"stock-number\");\n    if (!stockEl) return;\n\n    const key = getKey(variant);\n    let value = sessionStorage.getItem(key);\n\n    if (!value) {\n      value = String(Math.floor(Math.random() * (MAX_STOCK - MIN_STOCK + 1)) + MIN_STOCK); // 2–5\n      sessionStorage.setItem(key, value);\n    }\n\n    stockEl.textContent = value;\n  }\n\n  document.addEventListener(\"DOMContentLoaded\", update);\n\n  document.addEventListener(\"click\", function(e) {\n    if (e.target.closest(\".swatch-selector\")) setTimeout(update, 30);\n  });\n\n  document.addEventListener(\"change\", function(e) {\n    if (e.target && (e.target.matches(\"select\") || e.target.matches('input[type=\"radio\"]'))) {\n      setTimeout(update, 30);\n    }\n  });\n\n  document.addEventListener(\"variant:changed\", function() {\n    setTimeout(update, 0);\n  });\n})();\n</script>"
          }
        },
        "custom_dVXJXy": {
          "type": "custom",
          "disabled": true,
          "settings": {
            "code": "<!-- ✅ Dynamic Stock Indicator with Discount -->\n<div class=\"stock-indicator\">\n  <span class=\"dot\"></span>\n  <span>\n    Only <strong id=\"stock-number\"></strong> items left with \n    <strong>{{ saved_amount }}</strong> off!\n  </span>\n</div>\n\n<style>\n.stock-indicator {\n  display: flex;\n  align-items: center;\n  font-family: inherit;\n  color: #000;\n  font-size: 14px;\n  font-weight: 500;\n}\n\n.stock-indicator .dot {\n  width: 8px;\n  height: 8px;\n  background-color: #e60000;\n  border-radius: 50%;\n  margin-right: 8px;\n  animation: blink 2.5s infinite;\n}\n\n@keyframes blink {\n  0%, 80%, 100% { opacity: 1; }\n  40% { opacity: 0.2; }\n}\n</style>\n\n<script>\n// Start value\nconst START_VALUE = 6;\nconst MIN_VALUE = 2;\n\n// Retrieve current value or start at 6\nlet currentStock = localStorage.getItem(\"dynamicStock\");\n\nif (currentStock === null) {\n  currentStock = START_VALUE; // First time\n} else {\n  currentStock = Math.max(MIN_VALUE, parseInt(currentStock) - 1); // Decrease by 1 on refresh\n}\n\n// Save the new value\nlocalStorage.setItem(\"dynamicStock\", currentStock);\n\n// Display on page\ndocument.getElementById(\"stock-number\").textContent = currentStock;\n</script>"
